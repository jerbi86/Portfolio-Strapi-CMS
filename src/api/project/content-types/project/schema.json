{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Projects"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "tags": {
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    },
    "externalLink": {
      "type": "component",
      "component": "shared.button",
      "repeatable": false
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    }
  }
}
